source ~/dotfiles/config/nvim/init.vim 

set visualbell

let g:mapleader = ","

map <leader>t :action RenameElement<CR>
map <leader>r :action Run<CR>
map <leader>e :action Debug<CR>

map = :action ReformatCode<CR>

map <leader>d "_d
map <leader>D "_D

map <Left> :action Back<CR>
map <Right> :action Forward<CR>

nmap gi :action GotoImplementation<CR>

set surround
set clipboard+=unnamed
set idearefactormode=keep
